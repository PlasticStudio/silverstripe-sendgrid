{
  "name": "vulcandigital/silverstripe-sendgrid",
  "description": "A module to assist developers in sending template emails via SendGrid",
  "type": "silverstripe-vendormodule",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Reece Alexander",
      "email": "reece@vulcandigital.co.nz"
    }
  ],
  "require": {
    "silverstripe/framework": "^4.0",
    "sendgrid/sendgrid": "~8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "squizlabs/php_codesniffer": "3.*"
  },
  "autoload": {
    "psr-4": {
      "Vulcan\\SendGrid\\": "src/",
      "Vulcan\\SendGrid\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "extra": {
    "branch-alias": {
      "dev-master": "1.2.x-dev"
    }
  }
}
